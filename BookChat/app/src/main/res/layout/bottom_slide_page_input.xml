<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/background_bottom_slide_page_input"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="50dp"
        android:src="@drawable/ic_blue_book_mark"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView29"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_bold"
        android:text="책갈피"
        android:textColor="#222222"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView3"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginStart="100dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="100dp"
        app:cardCornerRadius="10dp"
        app:cardElevation="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29">

        <EditText
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp"
            android:background="@null"
            android:elegantTextHeight="true"
            android:fontFamily="@font/notosanskr_regular"
            android:gravity="center"
            android:inputType="number"
            android:maxLength="5"
            android:textColor="#222222"
            android:focusable="false"
            android:textCursorDrawable="@drawable/edittext_cursor"
            android:textSize="22sp" />

    </androidx.cardview.widget.CardView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="25dp"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:text="P"
        android:textColor="#4C4C4C"
        android:textSize="22sp"
        app:layout_constraintBottom_toBottomOf="@+id/cardView3"
        app:layout_constraintStart_toEndOf="@+id/cardView3"
        app:layout_constraintTop_toTopOf="@+id/cardView3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="1"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardView3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="2"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton3"
        app:layout_constraintStart_toEndOf="@+id/materialButton"
        app:layout_constraintTop_toBottomOf="@+id/cardView3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="3"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/materialButton2"
        app:layout_constraintTop_toBottomOf="@+id/cardView3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="4"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialButton" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="5"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton3"
        app:layout_constraintStart_toEndOf="@+id/materialButton"
        app:layout_constraintTop_toBottomOf="@+id/materialButton2" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="6"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/materialButton2"
        app:layout_constraintTop_toBottomOf="@+id/materialButton3" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton7"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="7"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialButton4" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton8"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="8"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton3"
        app:layout_constraintStart_toEndOf="@+id/materialButton"
        app:layout_constraintTop_toBottomOf="@+id/materialButton5" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="9"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/materialButton2"
        app:layout_constraintTop_toBottomOf="@+id/materialButton6" />


    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton10"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="?android:selectableItemBackground"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:stateListAnimator="@null"
        android:text="확인"
        android:textColor="#4C4C4C"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/materialButton11"
        app:layout_constraintEnd_toStartOf="@+id/materialButton2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/materialButton8" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton11"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="0"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toStartOf="@+id/materialButton3"
        app:layout_constraintStart_toEndOf="@+id/materialButton"
        app:layout_constraintTop_toBottomOf="@+id/materialButton8" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/materialButton12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:backgroundTint="#FFFFFF"
        android:elegantTextHeight="true"
        android:fontFamily="@font/notosanskr_regular"
        android:background="?android:selectableItemBackground"
        android:stateListAnimator="@null"
        android:text="←"
        android:textColor="#222222"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/materialButton2"
        app:layout_constraintTop_toBottomOf="@+id/materialButton9" />

</androidx.constraintlayout.widget.ConstraintLayout>