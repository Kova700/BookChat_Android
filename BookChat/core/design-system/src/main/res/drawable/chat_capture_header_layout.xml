<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <stroke
                android:width="1.5dp"
                android:color="#5648FF"
                android:dashWidth="3dp"
                android:dashGap="3dp" />
        </shape>
    </item>
    <item
        android:left="1.5dp"
        android:right="1.5dp"
        android:top="1.5dp">
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
</layer-list>